{
  "name": "@bounty/dev-logger",
  "version": "1.0.0",
  "description": "Development logging utility for bounty.new workspace",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run clean",
    "lint": "echo 'No linting configured'"
  },
  "keywords": [
    "logging",
    "development",
    "bounty",
    "typescript"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
